{
    "functions": {
        "Index.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/Index.php"
        }
    ],
    "builds": [
        {
            "src": "Index.php",
            "use": "@vercel/php"
        }
    ]
}